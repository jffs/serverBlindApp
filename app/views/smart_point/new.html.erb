<%= form_for :smart_point, :url => { :action => :create } do |f| %>
  <p>
    <%= f.label :macaddress %><br>
    <%= f.text_field :macaddress %>
  </p>
<p>
    <%= f.label :lat %><br>
    <%= f.text_field :lat %>
  </p><p>
    <%= f.label :long %><br>
    <%= f.text_field :long %>
  </p><p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :user_mac %><br>
    <%= f.text_field :user_mac %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>